{
  "permissions": {
    "allow": [
      "Bash(npm install:*)",
      "WebSearch",
      "WebFetch(domain:docusaurus.io)",
      "Bash(yarn docusaurus clear:*)",
      "Bash(npx docusaurus-mdx-checker:*)",
      "Bash(yarn build)",
      "Bash(for file in blog/*.md)",
      "Bash(do echo \"=== $file ===\")",
      "Bash(head:*)",
      "Bash(done)",
      "Bash(git log:*)",
      "Bash(git commit:*)"
    ],
    "deny": [],
    "ask": []
  },
  "sandbox": {
    "autoAllowBashIfSandboxed": true,
    "enabled": true
  }
}
